<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>

<AnchorPane id="AnchorPane" fx:id="mainPanel" prefHeight="400.0" prefWidth="600.0" styleClass="mainFxmlClass" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8.0.171" fx:controller="addingUser.AddingUserController">
    <stylesheets>
        <URL value="@addinguser.css" />
    </stylesheets>
   <children>
      <TextField fx:id="userId" alignment="CENTER" layoutX="226.0" layoutY="84.0" onKeyReleased="#dataInputKontrol" promptText="User ID" />
      <TextField fx:id="userName" alignment="CENTER" layoutX="226.0" layoutY="146.0" promptText="User Name" />
      <TextField fx:id="userSurname" alignment="CENTER" layoutX="226.0" layoutY="208.0" promptText="User Surname" />
      <HBox layoutY="300.0" prefHeight="100.0" prefWidth="600.0">
         <children>
            <Button fx:id="addButton" mnemonicParsing="false" onAction="#addUser" prefHeight="100.0" prefWidth="303.0" text="add User" />
            <Button fx:id="cancelButton" layoutX="10.0" layoutY="10.0" mnemonicParsing="false" onAction="#cancel" prefHeight="100.0" prefWidth="301.0" text="cancel" />
         </children>
      </HBox>
   </children>
</AnchorPane>
